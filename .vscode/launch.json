			"name": "测试命令行_1",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/downloader.py",
			"console": "integratedTerminal"
		},
		{
			"name": "测试命令行_2",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/downloader.py",
			"console": "integratedTerminal",
			"args": [
				"test_cmd"
			]
		},
		{
			"name": "临时测试",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/building.py",
			"console": "integratedTerminal"
		},
		{
			"name": "运行测试",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/downloader.py",
			"console": "integratedTerminal",
